    <script type="text/javascript">
    
     
    
        $(document).ready(function() {

          
                    $.ajax({
                        url: "/umbraco/api/General/SetPageViewCounter",
                        dataType: "JSON",
                        type: "POST",
                        data: {
                        contentId: 3932,
                         culture:"en-US"
                        },
                        success: function(data) {
                        }
                    });
            GetWeatherData();
         
