                                                                                            ${accessibilityContent}
                                                                                            ${familyWithChildrenContent}
                                                                                            ${over65Content}
                                                                                            ${warningsContent}
                                                                                            ${descriptionsContent}
                                                                                            ${igapassContent}
                                                                                        </div>
                                                                                    </div>
                                                                                </div>`;
                                            titleContent += `<li>
                                                            <a href="#item-${i + 1}" data-id="item-${i + 1}">
                                                                <span class="number">${i + 1}</span><span class="title ${result.data[i].activeClass}">${result.data[i].name}</span>
                                                            </a>

                                                        </li>`;
                                        }
                                        $("#row-" + pageId).append(guideContent);
                                        $("#list-" + pageId).append(titleContent);
                                        titleActive();
                                    }

                                }
                            });
                    }
                                                                                         
          var queryParams = new URLSearchParams(window.location.search);
          
          var hashUrl= parent.location.hash;
          if (hashUrl!=undefined && hashUrl!="")
          {
           var aNav=$(".nav-item a[href='"+hashUrl+"']");
           
                     $(".filter-check").each(function() {
                         $(aNav).prop("checked", false);
                     });
                     $(".guide-result").remove();
                     $(".fixed-aside").remove();
                     var pageId = $(aNav).data("id");
                     if (pageId != 0) {
                     console.log(pageId);
                         GetLocationInfo(pageId);
                     }
              
        
          }
          else
          {
           var aNav=$(".nav-item a").first();
             $(".filter-check").each(function() {
                         $(aNav).prop("checked", false);
                     });
                     $(".guide-result").remove();
                     $(".fixed-aside").remove();
                     var pageId = $(aNav).data("id");
                     if (pageId != 0) {
                     console.log(pageId);
                         GetLocationInfo(pageId);
                     }
          }
           
                         $(".nav-link").on("click", function() {

                              
                                 queryParams.set("tab", $(this).attr("data-index"));
        
queryParams.set("locale", "fr");         
                                   history.replaceState(null, null, "?" + queryParams.toString());
                                   parent.location.hash = $(this).attr("aria-controls");
                                   
                                      hashUrl= parent.location.hash;
                                 
                                 $(".tab-pane").removeClass("active").removeClass("show");
                                 $("."+hashUrl.replace("#","")+"-tabs").addClass("active").addClass("show");
                               

                           });
                                                                              $(function() {
                                                                                             $(document).on("click", ".deselect-all", function() {
                                                                                                 var dataId = $(this).data("id");
                                                                                                 $("." + dataId + " " + ".default-checkbox input").each(function(index) {
                                                                                                     $(this).prop("checked", false);
                                                                                                     var parentId = $(this).closest(".default-checkbox").attr("id");
                                                                                                     CustomizedContentControl($(this), parentId);
                                                                                                 });
                                                                                             });
                                                                                         });
                                                                                         $(function() {
                                                                                             $(document).on("click", ".filter-apply", function(e) {
                                                                                                 $(".filter-menu").removeClass("open");
                                                                                                 e.preventDefault();
                                                                                                 var filterId = $(this).attr("id");
                                                                                                 var input = $(this).children(".filter-check");
                                                                                                 CustomizedContentControl(input, filterId);
                                                                                             });
                                                                                         });
                                                                                         $(function() {
                                                                                             $(document).on("click", ".close-filter", function() {
                                                                                                 var dataId = $(this).data("id");
                                                                                                 $("." + dataId + " " + ".default-checkbox input").each(function(index) {
                                                                                                     $(this).prop("checked", false);
                                                                                                     var parentId = $(this).closest(".default-checkbox").attr("id");
                                                                                                     CustomizedContentControl($(this), parentId);
                                                                                                 });
                                                                                             });
                                                                                         });
    </script>
<script type="text/javascript">
                                                                                        // var pageId = '1927';
                                                                                         //if (pageId != 0) {
                                                                                            // GetLocationInfo();
                                                                                         //}

                                                                                         $(function() {
                                                                                             $(document).on("change", ".default-checkbox", function(e) {
                                                                                                 e.preventDefault();
                                                                                                 var filterId = $(this).attr("id");
                                                                                                 var input = $(this).children(".filter-check");
                                                                                                 CustomizedContentControl(input, filterId);
                                                                                             });
                                                                                         });


                                                                                         function CustomizedContentControl(input, filterId) {

                                                                                             if (!$(input).closest(".default-checkbox").hasClass("all-filter")) {
                                                                                                 if ($(input).is(":checked")) {
                                                                                                     $("." + filterId).each(function(index) {
                                                                                                         $(this).removeClass("d-none").addClass("filter-result");

                                                                                                         $(".filter-row").each(function(index) {

                                                                                                             var resultCount = $(this).children(".filter-result");
                                                                                                             if (resultCount.length === 0) {
                                                                                                                 $(this).children(".accesibility-active").addClass("d-none");
                                                                                                             }
                                                                                                             else {
                                                                                                                 $(this).children(".accesibility-active").removeClass("d-none");
                                                                                                             }
                                                                                                         });
                                                                                                     });
                                                                                                 }
                                                                                                 else {
                                                                                                     $("." + filterId).each(function(index) {
                                                                                                         $(this).addClass("d-none").removeClass("filter-result");

                                                                                                         $(".filter-row").each(function(index) {
                                                                                                             var resultCount = $(this).children(".filter-result");
                                                                                                             if (resultCount.length === 0) {
                                                                                                                 $(this).children(".accesibility-active").addClass("d-none");
                                                                                                             }
                                                                                                             else {
                                                                                                                 $(this).children(".accesibility-active").removeClass("d-none");
                                                                                                             }
                                                                                                         });
                                                                                                     });
                                                                                                 }
                                                                                             }
                                                                                             else {

                                                                                                 if ($(input).is(":checked")) {
                                                                                                     $(".sub-check").each(function() {
                                                                                                         $(this).prop("checked", true);

                                                                                                         $(".filter-row .custom-filter").each(function(index) {

                                                                                                             $(this).removeClass("d-none").addClass("filter-result");
                                                                                                             $(".filter-row").each(function(index) {

                                                                                                                 var resultCount = $(this).children(".filter-result");
                                                                                                                 if (resultCount.length === 0) {
                                                                                                                     $(this).children(".accesibility-active").addClass("d-none");
                                                                                                                 }
                                                                                                                 else {
                                                                                                                     $(this).children(".accesibility-active").removeClass("d-none");
                                                                                                                 }
                                                                                                             });
                                                                                                         });
                                                                                                     });

                                                                                                 }
                                                                                                 else {
                                                                                                     $(".filter-check").each(function() {
                                                                                                         $(this).prop("checked", false);

                                                                                                         $(".filter-row .filter-result").each(function(index) {
                                                                                                             $(this).addClass("d-none").removeClass("filter-result");
                                                                                                             $(".filter-row").each(function(index) {

                                                                                                                 var resultCount = $(this).children(".filter-result");
                                                                                                                 if (resultCount.length === 0) {
                                                                                                                     $(this).children(".accesibility-active").addClass("d-none");
                                                                                                                 }
                                                                                                                 else {
                                                                                                                     $(this).children(".accesibility-active").removeClass("d-none");
                                                                                                                 }
                                                                                                             });
                                                                                                         });
                                                                                                     });
                                                                                                 }

                                                                                             }

                                                                                         }
                                                                                         $(function() {
                                                                                             $(document).on("click", ".nav-item a", function() {
                                                                                                 $(".filter-check").each(function() {
                                                                                                     $(this).prop("checked", false);
                                                                                                 });
                                                                                                 $(".guide-result").remove();
                                                                                                 $(".fixed-aside").remove();
                                                                                                 var pageId = $(this).data("id");
                                                                                                 if (pageId != 0) {
                                                                                                     GetLocationInfo(pageId);
                                                                                                 }
                                                                                             });
                                                                                         });

                                                                                        


                                                                                         function titlesSlideUp() {
                                                                                             $('.passenger-guide .content-titles').removeClass('open');

                                                                                             $('.passenger-guide .content-titles ul').slideUp();
                                                                                         }

                                                                                         function heightEqualization() {
                                                                                             const titlesHeight = $('.passenger-guide .fixed-aside').height();
                                                                                             const guideResult = $('.passenger-guide .guide-result');
                                                                                             if (titlesHeight > 500) {
                                                                                                 guideResult.css({ 'max-height': titlesHeight });
                                                                                             }
                                                                                         }
                                                                                         function itemTitlesFixed() {
                                                                                             if ($(window).width() < 992) {
                                                                                                 const topDist = $(".passenger-guide .content-titles").offset();
                                                                                                 $(window).on('scroll', function() {
                                                                                                     const scroll = $(window).scrollTop();
                                                                                                     const titlesWidth = $(".passenger-guide .content-titles").width();
                                                                                                     if (topDist != null) {
                                                                                                         if (scroll > topDist.top) {
                                                                                                             $('.passenger-guide .content-titles').css({ "position": "fixed", "top": "0", "width": titlesWidth, 'z-index': '2' }).addClass('fixed');
                                                                                                         } else {
                                                                                                             $('.passenger-guide .content-titles').css({ "position": "static", "top": "auto", "width": 'auto' }).removeClass('fixed');
                                                                                                         }
                                                                                                     }
                                                                                                 });

                                                                                             }

                                                                                         }

                                                                                         function titleActive() {

                                                                                             $('.passenger-guide .guide-result .item').each(function(key, item) {
                                                                                                 if ($(window).width() < 992) {
                                                                                                     $(window).on('scroll', function() {

                                                                                                         const itemHeight = $(item).height();
                                                                                                         const scrollPosition = $(window).scrollTop();
                                                                                                         const itemOffsetTop = $(item).offset().top;
                                                                                                         if (scrollPosition > itemOffsetTop && scrollPosition < itemOffsetTop + itemHeight) {
                                                                                                             $('.passenger-guide .content-titles ul li a .title').removeClass('active');
                                                                                                             const currentId = $(item).attr('id');
                                                                                                             $(`.passenger-guide .content-titles ul li a[data-id=${currentId}] .title`).addClass('active');
                                                                                                         }
                                                                                                     });

                                                                                                 } else {
                                                                                                     $('.passenger-guide .guide-result').on('scroll', function() {
                                                                                                         const itemHeight = $(item).height();
                                                                                                         const scrollPosition = $('.passenger-guide .guide-result').scrollTop();
                                                                                                         const itemOffsetTop = $(item).position().top;
                                                                                                         if (scrollPosition > itemOffsetTop && scrollPosition < itemOffsetTop + itemHeight) {
                                                                                                             $('.passenger-guide .content-titles ul li a .title').removeClass('active');
                                                                                                             const currentId = $(item).attr('id');
                                                                                                             $(`.passenger-guide .content-titles ul li a[data-id=${currentId}] .title`).addClass('active');
                                                                                                         }
                                                                                                     });
                                                                                                 }


                                                                                             });

                                                                                         }


                                                                                         $(document).ready(function() {


                                                                                             if ($(window).width() < 992) {
                                                                                                 titlesSlideUp();
                                                                                                 $(document).on('click', '.passenger-guide .content-titles .mobile-open', function() {
                                                                                                     if ($('.passenger-guide .content-titles').hasClass('open')) {
                                                                                                         titlesSlideUp();
                                                                                                     } else {
                                                                                                         $('.passenger-guide .content-titles').addClass('open');
                                                                                                         $('.passenger-guide .content-titles ul').slideToggle();

                                                                                                     }

                                                                                                 });
                                                                                             }

                                                                                             titleActive();
                                                                                             itemTitlesFixed();
                                                                                             heightEqualization();

                                                                                             $(window).on('resize', function() {
                                                                                                 if ($(window).width() > 991) {
                                                                                                     heightEqualization();
                                                                                                 }
                                                                                             });



                                                                                         });

                                                                                         $(document).on('click', '.content-titles ul li a', function() {

                                                                                             var id = $(this).attr('data-id');
                                                                                             if ($(window).width() < 992) {
                                                                                                 const itemHeight = $(`.guide-result .item#${id}`).offset().top;
                                                                                                 const titlesHeight = $('.passenger-guide .fixed-aside ul').height();

                                                                                                 if ($('.passenger-guide .fixed-aside .content-titles').hasClass('fixed')) {
                                                                                                     $('html, body').animate({
                                                                                                         scrollTop: itemHeight
                                                                                                     }, 0);
                                                                                                 } else {
                                                                                                     $('html, body').animate({
                                                                                                         scrollTop: itemHeight - (titlesHeight * 2)
                                                                                                     }, 0);
                                                                                                 }


                                                                                                 titlesSlideUp()
                                                                                             } else {
                                                                                                 $(".guide-result").stop();
                                                                                                 const scrollHeight = $('.guide-result').scrollTop();
                                                                                                 const itemHeight = $(`.guide-result .item#${id}`).position().top;

                                                                                                 $('.guide-result').animate({
                                                                                                     scrollTop: scrollHeight + itemHeight - 60
                                                                                                 }, 1500);

                                                                                             }


                                                                                         });
                                                                                         $(document).on('click', '.passenger-guide .filter-open', function() {
                                                                                             $(".passenger-guide .filter-menu").addClass('open');
                                                                                         });
                                                                                         $(document).on('click', '.passenger-guide .close-filter', function() {
                                                                                             $(".passenger-guide .filter-menu").removeClass('open');
                                                                                         });
     

    </script>
<script>

                 $(function() {
                     $("#txtSearch").on('paste', function(e) {
                         var _this = $(this);
                         // we can access input value on paste after some time
                         setTimeout(function() {
                             // access element value
                             var txtLength = _this.val().length;
                             if (txtLength >= 3) {
                                 if ($('.search-card-slick .slick-list').length > 0) {
                                     $('.search-card-slick').slick('destroy');
                                 }

                                 GetPopulerSearches();
                                 GetHeaderSearchResult();
                             }
                             else {
                                 $("#populer-searches-template").empty();
                                 $("#related-categories-template").empty();
                                 $("#card-result-template").empty();
                                 $(".see-all-populer").empty();
                                 $(".see-all-related").empty();
                                 $(".category-title-result span").empty();
                                 $(".populer-searches-title span").empty();
                             }
                         }, 1);


                     })



                     $("#txtSearch").on('keydown', function(e) {

                         var txtLength = $(this).val().length;
                         var code = e.keyCode ? e.keyCode : e.which;

                         if (code == 13) {
                             RedirectToSearchPage();


                         }
                         else if (txtLength >= 3) {
                             if ($('.search-card-slick .slick-list').length > 0) {
                                 $('.search-card-slick').slick('destroy');
                             }

                             GetPopulerSearches();
                             GetHeaderSearchResult();
                         }
                         else if (txtLength < 3) {
                             $("#populer-searches-template").empty();
                             $("#related-categories-template").empty();
                             $("#card-result-template").empty();
                             $(".see-all-populer").empty();
                             $(".see-all-related").empty();
                             $(".category-title-result span").empty();
                             $(".populer-searches-title span").empty();
                         }
                     })
                 });


                 function GetPopulerSearches() {

                     $.ajax({
                         type: "GET",
                         url: "/umbraco/Surface/Search/GetPopulerSearches",
                         contentType: "application/json; charset=utf-8",
                         data: { culture: "en-US" },
                         success: function(result) {
                             if (result != null) {

                                 var seeAllText = 'See All';

                                 $("#populer-searches-template").html("");
                                 //$('.populer-searches-title').html("<span>Popler Arama</span>");
                                 $('.populer-searches-title').html("Populer Searches");
                                 if (result.categories.length > 0) {

                                     let relCatTemplate = "";
                                     $('.see-all-populer').html('<a href="javascript:;" onclick="RedirectToPopulerSearchPage()"><span class="text">' + seeAllText + '</span><span class="icon icon-chevron-right"></span></a>');
                                     $.each(result.categories, function(index, value) {

                                         if (value.category != '') {
                                             relCatTemplate += `<li><a href="#">${value.category} (${value.resultList.length})</a></li>`
                                         }
                                     });

                                     $("#populer-searches-template").append(relCatTemplate);
                                 }
                                 else {

                                     $('#populer-searches-template').empty();

                                     $('.populer-searches-title span').empty();

                                     $('.see-all-populer').empty();
                                 }
                             }
                         }
                     });
                 }

                 function GetHeaderSearchResult() {

                     var searchKey = $.trim($("#txtSearch").val());

                     var culture = 'en-US';

                     var searchPage = '/en/search/';

                     var seeAllText = 'See All';
                     var readDetails = 'Details';


                     $.ajax({
                         type: "GET",
                         url: "/umbraco/Surface/Search/HeaderSearch",
                         contentType: "application/json; charset=utf-8",
                         data: { searchKey: searchKey, culture: culture },
                         success: function(result) {

                             $('.category-title-result span').empty();

                             $('.see-all-related').empty();

                             if (result != null) {

                                 $("#related-categories-template").html("");
                                 if (result.categories.length > 0) {
                                     $(".search-result").find(".result-item").show();
                                     $(".search-result").find(".no-result").hide();

                                     let relCatTemplate = "";

                                     $('.category-title-result span').text("Related Categories");

                                     $('.see-all-related').append(`<a href="javascript:;" onclick="RedirectToSearchPage()"><span class="text">${seeAllText}</span><span class="icon icon-chevron-right"></span></a>`);


                                     $.each(result.categories, function(index, value) {

                                         if (value.category != '') {
                                             relCatTemplate += `<li><a href="#">${value.category} (${value.resultList.length})</a></li>`
                                         }
                                     });

                                     $("#related-categories-template").append(relCatTemplate);

                                 }
                                 else {

                                     $(".search-result").find(".result-item").hide();
                                     $(".search-result").find(".no-result").show();

                                     $('#related-categories-template').empty();

                                     $('.category-title-result span').empty();

                                     $('.see-all-related').empty();
                                 }

                                 if (result.recommended.length > 0) {

                                     let recTemplate = "";

                                     $('#card-result-template').empty();

                                     $.each(result.recommended, function(index, value) {

                                         var tag = value.tag;
                                         var title = value.title;
                                         var contentType = value.contentType;

                                         if (title == null || title == "") {
                                             title = value.name;
                                         }

                                         recTemplate += `<div class="result-item">
                                                                 <div class="default-card">
                                                                     <div class="card-content">
                                                                         <div class="content-body">
                                                                             <div class="category-title">
                                                                             <span>${tag}</span>
                                                                             </div>
                                                                             <div class="main-title">
                                                                                 <span>${title}</span>
                                                                             </div>`;
                                         if (contentType == "blogPage") {
                                             recTemplate += `<div class="icons d-flex mt-3">
                                                                                                         <div class="view mr-3">
                                                                                                         <span class="icon-eye"></span>
                                                                                                         <span class="text">${value.viewCount}</span>
                                                                                                         </div>
                                                                                                     </div>`;
                                         }
                                         recTemplate += ` </div>
                                                                         <div class="content-footer">
                                                                         <div class="read-details black">
                                                                                 <a href="${value.url}"><span class="text">${readDetails}</span><span class="icon icon-chevron-right"></span></a>
                                                                         </div>
                                                                         </div>
                                                                     </div>
                                                                     <div class="card-img"><img src="${value.imageUrl}?format=webp" alt="" />
                                                                     </div>
                                                                 </div>
                                                                 </div>`;

                                     });

                                     $('#card-result-template').append(recTemplate);

                                     $('.search-card-slick').not('.slick-initialized').slick({
                                         dots: false,
                                         infinite: false,
                                         variableWidth: true,
                                         arrows: false,
                                         swipe: true,
                                         responsive: [{
                                             breakpoint: 980,
                                             settings: {
                                                 swipe: true
                                             }
                                         }]
                                     });
                                     $('.search-card-slick').slick('refresh');
                                     //if ($(window).width() < 767) {
                                     //    $('.search-carousel').slick({
                                     //        dots: false,
                                     //        infinite: false,
                                     //        variableWidth: true,
                                     //        slidesToShow: 2,
                                     //        slidesToScroll: 2,
                                     //        arrows: false,
                                     //        swipe: false

                                     //    });
                                     //}
                                 }
                                 else {
                                     $('#card-result-template').empty();
                                 }
                             }
                             else {

                                 $(".search-result").find(".result-item").hide();
                                 $(".search-result").find(".no-result").show();
                                 $('#related-categories-template').empty();

                                 $('#card-result-template').empty();
                             }
                             setTimeout(function() {

                             }, 1000)

                         }
                     });



                 }

                 function RedirectToSearchPage() {

                     var searchKey = $.trim($("#txtSearch").val());

                     var searchPage = '/en/search/';

                     window.location.href = searchPage + "?searchKey=" + searchKey;

                 }

                 function RedirectToPopulerSearchPage() {

                     var searchPage = '/en/search/';
                     window.location.href = searchPage;

                 }

    </script>
