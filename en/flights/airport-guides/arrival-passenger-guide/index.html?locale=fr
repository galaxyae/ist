    <script type="text/javascript">
    
     
    
        $(document).ready(function() {

          
                    $.ajax({
                        url: "/umbraco/api/General/SetPageViewCounter",
                        dataType: "JSON",
                        type: "POST",
                        data: {
                        contentId: 1907,
                         culture:"en-US"
                        },
