<script type="text/javascript">
      
        function GetLocationInfo(pageId) {

                        var culture = 'en-US';

                        $.ajax(
                            {
                                url: "/umbraco/surface/PassengerGuide/GetPassengerGuideLocationInfo",
                                type: "GET",
                                dataType: "json",
                                data: { pageId: pageId, culture: culture },
                                contentType: "application/json; charset=utf-8",
                                success: function(result) {
                                    var guideContent = "";
                                    var titleContent = "";
                                    if (result.success) {
                                        var fileContent = "";
                                        var guideResult = `<div class="guide-result" id="row-${pageId}"></div>`;
                                        var listResult = `<div class="fixed-aside">
                                                            <div class="content-titles">
                                                                <div class="mobile-open">
                                                                    <span>Content Titles</span>
                                                                    <span class="icon-chevron-down icon"></span>
                                                                </div>
                                                                <ul  id="list-${pageId}"></ul>
                                                            </div>
                                                            <div class="download-file">
                                                                <div class="title">
                                                                    <span id="span-${pageId}"></span>
                                                                </div>
                                                                <div class="files" id="files-${pageId}">
                                                                </div> 
                                                            </div>
                                                        </div>`;
                                        $("#customRow-" + pageId).append(guideResult);
                                        $("#customList-" + pageId).prepend(listResult);
                                        if (result.files.files.length > 0) {
                                            for (var x = 0; x < result.files.files.length; x++) {
                                                fileContent += `<div class="file"><a href="${result.files.files[x].fileUrl}">
                                                <span class="icon-download icon"></span> <span class="file-name">${result.files.files[x].text}</span></a></div>`;
                                            }
                                            $("#files-" + pageId).append(fileContent);
                                        }

                                        for (var i = 0; i < result.data.length; i++) {
                                            $("#span-" + pageId).text(result.files.fileText);
                                            var linkContent = "";
                                            var accessibilityContent = "";
                                            var accordionList = "";
                                            var familyWithChildrenContent = "";
                                            var over65Content = "";
                                            var warningsContent = "";
                                            var descriptionsContent = "";
                                            var igapassContent = "";
                                            if (result.data[i].link != "") {
                                                linkContent = `<div class="view-360">
                                                    <a href="${result.data[i].link}" target="${result.data[i].linkNewTab}">
                                                        <span class="icon icon-horizontal-360-deg"></span>
                                                    </a>
                                                </div>`;
                                            }

                                            if (result.data[i].accordionList.length > 0)
                                            {
                                                accordionList += `<div class="custom-filter  mt-5">`;
                                                for (var a = 0; a < result.data[i].accordionList.length; a++) {
                                                    var accListItem = result.data[i].accordionList[a];
                                                accordionList += ` <div class="circle-title">
                                                            <span>
                                                                ${accListItem.title}
                                                            </span>
                                                        </div>`;

                                                    accordionList+= `<div id="accordion${i}${a}" class="accordion">`;
                                                    console.log(a);
                                                    if (accListItem.accordionItem != null) {
                                                        for (var b = 0; b < accListItem.accordionItem.length; b++) {
                                                            var accItem = accListItem.accordionItem[b];
                                                            accordionList += `<div class="card">
                                                                <div class="card-header" id="headingTwo">
                                                                    <h5 class="mb-0">
                                                                    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse${i}${b}${a}"  aria-expanded="false"  aria-controls="collapse${i}${b}${a}">
                                                                        <span class="title"> ${accItem.title} </span>
                                                                        <div class="icon">
                                                                        <span class="icon-chevron-down"></span>
                                                                        <span class="icon-chevron-up"></span>
                                                                        </div>
                                                                    </button>
                                                                    </h5>
                                                                </div>
                                                                <div  id="collapse${i}${b}${a}" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion${i}${a}">
                                                                    <div class="card-body">
                                                                        ${accItem.description}
                                                                    </div>
                                                                </div>
                                                                </div>`;
                                                        }
                                                    }
                                                    accordionList += ` </div>`;
                                                }
                                                accordionList += ` </div>`;
                                            }

                                            if (result.data[i].accessibility.length > 0) {
                                                accessibilityContent += `<div class="custom-filter d-none accessibility-filter-${pageId} mt-5">
                                                                            <div class="circle-title">
                                                                            <span>
                                                                                ${result.data[i].accessibilityTitle}
                                                                            </span>
                                                                            </div>
                                                                        <div id="accordion-accessibility-${i}" class="accordion">`;

                                                         for (var a = 0; a < result.data[i].accessibility.length; a++) {

                                                            var accItem =  result.data[i].accessibility[a];
                                                            accessibilityContent += `<div class="card">
                                                                <div class="card-header" id="headingTwo">
                                                                    <h5 class="mb-0">
                                                                    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse${i}-accessibility-${a}"  aria-expanded="false"  aria-controls="collapse${i}-accessibility-${a}">
                                                                        <span class="title"> ${accItem.title} </span>
                                                                        <div class="icon">
                                                                        <span class="icon-chevron-down"></span>
                                                                        <span class="icon-chevron-up"></span>
                                                                        </div>
                                                                    </button>
                                                                    </h5>
                                                                </div>
                                                                <div  id="collapse${i}-accessibility-${a}" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion-accessibility-${i}">
                                                                    <div class="card-body">
                                                                        ${accItem.description}
                                                                    </div>
                                                                </div>
                                                                </div>`;
                                                       }
                                                   accessibilityContent += ` </div></div>`;


                                            }
                                            if (result.data[i].familyWithChildren.length > 0) {
                                               
                                                     familyWithChildrenContent += `<div class="custom-filter d-none family-filter-${pageId} mt-5">
                                                                    <div class="circle-title">
                                                                    <span>
                                                                        ${result.data[i].familyWithChildrenTitle}
                                                                    </span>
                                                                    </div>
                                                                <div id="accordion-familyWithChildren-${i}" class="accordion">`;

                                                for (var a = 0; a < result.data[i].familyWithChildren.length; a++) {

                                                    var accItem = result.data[i].familyWithChildren[a];
                                                    familyWithChildrenContent += `<div class="card">
                                                                <div class="card-header" id="headingTwo">
                                                                    <h5 class="mb-0">
                                                                    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse${i}-familyWithChildren-${a}"  aria-expanded="false"  aria-controls="collapse${i}-familyWithChildren-${a}">
                                                                        <span class="title"> ${accItem.title} </span>
                                                                        <div class="icon">
                                                                        <span class="icon-chevron-down"></span>
                                                                        <span class="icon-chevron-up"></span>
                                                                        </div>
                                                                    </button>
                                                                    </h5>
                                                                </div>
                                                                <div  id="collapse${i}-familyWithChildren-${a}" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion-familyWithChildren-${i}">
                                                                    <div class="card-body">
                                                                        ${accItem.description}
                                                                    </div>
                                                                </div>
                                                                </div>`;
                                                            }
                                                       
                                                   familyWithChildrenContent += ` </div></div>`;

                                                }

                                            
                                            if (result.data[i].over65.length > 0) {
                                             
                                                over65Content += `<div class="custom-filter d-none old-filter-${pageId} mt-5">
                                                                    <div class="circle-title">
                                                                    <span>
                                                                        ${result.data[i].over65Title}
                                                                    </span>
                                                                    </div>
                                                                <div id="accordion-over65-${i}" class="accordion">`;

                                                        for (var a = 0; a < result.data[i].over65.length; a++) {

                                                            var accItem =  result.data[i].over65[a];
                                                            over65Content += `<div class="card">
                                                                <div class="card-header" id="headingTwo">
                                                                    <h5 class="mb-0">
                                                                    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse${i}-over65-${a}"  aria-expanded="false"  aria-controls="collapse${i}-over65-${a}">
                                                                        <span class="title"> ${accItem.title} </span>
                                                                        <div class="icon">
                                                                        <span class="icon-chevron-down"></span>
                                                                        <span class="icon-chevron-up"></span>
                                                                        </div>
                                                                    </button>
                                                                    </h5>
                                                                </div>
                                                                <div  id="collapse${i}-over65-${a}" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion-over65-${i}">
                                                                    <div class="card-body">
                                                                        ${accItem.description}
                                                                    </div>
                                                                </div>
                                                                </div>`;
                                                       }

                                                   over65Content += ` </div></div>`;

                                                
                                            }
                                            if (result.data[i].warnings.length > 0) {
                                              
                                                      warningsContent += `<div class="custom-filter d-none warnings-filter-${pageId} mt-5">
                                                                    <div class="circle-title">
                                                                    <span>
                                                                        ${result.data[i].warningsTitle}
                                                                    </span>
                                                                    </div>
                                                                <div id="accordion-warnings-${i}" class="accordion">`;

                                                        for (var a = 0; a < result.data[i].warnings.length; a++) {

