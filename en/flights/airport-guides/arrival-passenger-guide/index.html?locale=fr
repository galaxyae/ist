                                                            warningsContent += `<div class="card">
                                                                <div class="card-header" id="headingTwo">
                                                                    <h5 class="mb-0">
                                                                    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse${i}-warnings-${a}"  aria-expanded="false"  aria-controls="collapse${i}-warnings-${a}">
                                                                        <span class="title"> ${accItem.title} </span>
                                                                        <div class="icon">
                                                                        <span class="icon-chevron-down"></span>
                                                                        <span class="icon-chevron-up"></span>
                                                                        </div>
                                                                    </button>
                                                                    </h5>
                                                                </div>
                                                                <div  id="collapse${i}-warnings-${a}" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion-warnings-${i}">
                                                                    <div class="card-body">
                                                                        ${accItem.description}
                                                                    </div>
                                                                </div>
                                                                </div>`;
                                                       }

                                                   warningsContent += ` </div></div>`;

                                                

                                            }
                                            if (result.data[i].descriptions.length > 0) {
                                           
