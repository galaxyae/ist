    <script src="https://www.istairport.com/umbraco/lib/jquery/jquery.min.js"></script>
    <script src="https://www.istairport.com/assets/scripts/IGA-Istairport.lib.min.js?ver=1894712366"></script>
 
   
       <script src="https://www.istairport.com/assets/scripts/IGA-Istairport.app.min.js?ver=1280623293"></script>
    
    <script src="https://www.istairport.com/assets/lib/jquery.validate.js?ver=385528976"> </script>
  
    <script src="https://www.istairport.com/assets/lib/jquery.validate.unobtrusive.js"> </script>
        <script async src="https://cdn.mindbehind.com/sdk/mindbehind-sdk.js?auto=true&amp;key=65915fd0137300741ec04060"></script>
    <script src="https://www.istairport.com/assets/lib/jquery.mask.min.js"></script>
    <script src="https://www.istairport.com/assets/lib/sweetalert.min.js"></script>


    

    <script type="text/javascript">
             function ddMMM(dateIn) {

                 var mm = dateIn.toLocaleDateString("en-US", { month: 'long' });
                 var dd = dateIn.getDate();
                 return String(dd + " " + mm);
             }

             $(".placard").addClass("search-input");
             var flightType = 1;
             var flightLocationType = 1;
             var pageNumber = 1;
             var pageSize = 10;
             var searchTerm = "changeflight";
             var startDate = "";
             var endDate = "";
             var activeTab = $(".nav-link.active:first");
             var activeTabId = activeTab.attr("id");
             var currentTab = activeTabId == "departure-tab" ? 0 : 1;
             var prevFlightPage = 0;
             var clickedButton = "";
             var prevStartDate = "";

             if (flightType == 0) {
                 $(".counter").attr("style", "display:none");
                 $(".carousel").removeAttr("style");
             }
             else {
                 $(".counter").removeAttr("style");
                 $(".carousel").attr("style", "display:none");
             }


             $(document).ready(function () { GetFlightStatusBoard(0); });

             $(document).on("click", ".showPrevFlights", function () {
                 prevFlightPage = (prevFlightPage + 1);
                 //RefreshBoard();
                 clickedButton = "prevFlight";
                 startDate = prevStartDate;
                 GetFlightStatusBoard(0);

             });

             $(document).on("click", ".refreshBoard", function () {

                 $(".showPrevFlights").show();
                 $(".prevFlights").show();
                 searchTerm = ""; RefreshBoard(); GetFlightStatusBoard(0);

             });

             $(document).on("paste", ".flightSearchInput", function (e) {

                 $(".showPrevFlights").show();
                 $(".prevFlights").show();
                 var _this = $(this);
                 setTimeout(function () {
                     var txtLength = _this.val().length;
                     if (txtLength >= 3) {
                         searchTerm = _this.val();
                         RefreshBoard();
                         GetFlightStatusBoard(1);
                     }
                     else {
                         searchTerm = ""; RefreshBoard(); GetFlightStatusBoard(0);
                     }
                 }, 1);
             });

             $(document).on("keyup", ".flightSearchInput", function (e) {

                 $(".showPrevFlights").show();
                 $(".prevFlights").show();
                 var txtLength = $(this).val().length;
                 var code = e.keyCode ? e.keyCode : e.which;

                 if (txtLength >= 3) {
                     searchTerm = $(this).val();
                     RefreshBoard();
                     GetFlightStatusBoard(1);
                 }
                 else {
                     searchTerm = ""; RefreshBoard(); GetFlightStatusBoard(0);
                 }


             });
             $(document).on("click", "#departure-tab", function () {
                 if ($(".today").parent().hasClass("selected")) {
                     $(".showPrevFlights").show();
                     $(".prevFlights").show();
                 }

                 $(".flightSearchInput").val("");

                 currentTab = 0;
                 searchTerm = "";
                 pageNumber = 1;
                 isCurrentTime = true;
                 flightType = 1;
                 $(".table-header").find(".arrival").html('Arrival');
                 flightLocationType = 0;
                 RefreshBoard();
                 GetFlightStatusBoard(0);
             });

             $(document).on("click", "#arrival-tab", function () {

                 if ($(".today").parent().hasClass("selected")) {
                     $(".showPrevFlights").show();
                     $(".prevFlights").show();
                 }
                 $(".flightSearchInput").val("")
                 currentTab = 1;
                 searchTerm = "";
                 pageNumber = 1;
                 flightType = 0;
                 $(".table-header").find(".arrival").html('Departure');
                 flightLocationType = 0;
                 RefreshBoard();
                 GetFlightStatusBoard(0);
             });

             $(document).on("click", ".flightLocationType", function () {

                 pageNumber = 1;
                 if ($(".today").parent().hasClass("selected")) {
                     $(".showPrevFlights").show();
                     $(".prevFlights").show();
                 }

                 flightLocationType = $(this).val();// $(this).is(":checked") ? 1: 0;
                 RefreshBoard();
                 GetFlightStatusBoard(0);
             });

             $(document).on("click", ".showMoreFlights", function () {

                 pageNumber = pageNumber + 1;
                 clickedButton = "moreFlight";
                 //flightLocationType = $(this).is(":checked") ? ((int)FlightLocationType.International): ((int)FlightLocationType.Domestic);

                 GetFlightStatusBoard(0);
             });

             $(document).on("click", ".changeFlightDate", function () {


                 startDate = $(this).data("startdate");
                 endDate = $(this).data("enddate");
                 prevFlightPage = 0;

                 if (currentTab == 0) {
                     searchTerm = $("#depInput").val();
                 }
                 else {
                     searchTerm = $("#arrInput").val();
                 }

                 if ($(this).hasClass("today")) {
                     $(".showPrevFlights").show();
                     $(".prevFlights").show();
                     startDate = "";
                     endDate = "";
                 }
                 else {
                     $(".showPrevFlights").hide();
                     $(".prevFlights").hide();
                 }


                 $(".changeFlightDate").parent().removeClass("selected");
                 $(this).parent().addClass("selected");
                 //flightLocationType = $(this).is(":checked") ? ((int)FlightLocationType.International): ((int)FlightLocationType.Domestic);
                 RefreshBoard();
                 GetFlightStatusBoard(1);
             });

             function GetFlightStatusBoard(issearch) {

                 $("#loading").show();

                 $.ajax({
                     url: "/umbraco/api/FlightInfo/GetFlightStatusBoard",
                     dataType: "JSON",
                     type: "POST",
                     data: {
                         nature: flightType,
                         searchTerm: searchTerm,
                         pageNumber: pageNumber,
                         pageSize: pageSize,
                         isInternational: flightLocationType,
                         date: startDate,
                         endDate: endDate,
                         culture: "en",
                         prevFlightPage: prevFlightPage,
                         clickedButton: clickedButton
                     },
                     success: function (data) {
                         $('.airline-code-slick').slick('unslick');
                         $('.flight-code-slick').slick('unslick');

                         var noresultText = "No results were found matching your search. You must enter full flight number, city, etc. &#x130;nformation.";

                         if (data.status === true) {



                             if (data.result.data.flights.length < pageSize) {
                                 $(".moreFlights").hide();
                             }
                             else {
                                 if (!$(".moreFlights").is(":hidden"))
                                     $(".moreFlights").show();
                             }

                             if ((data.result.data.flights.length == 0 && clickedButton == "prevFlight") || data.result.lastPrevFlights) {
                                 $(".prevFlights").hide();
                             }
                             else {
                                 if (!$(".prevFlights").is(":hidden"))
                                     $(".prevFlights").show();
                             }

                             if (data.result.data.flights.length == 0 && pageSize < 1) {
                                 $(".showMoreFlights").hide();
                                 $(".no-result").html("<p class='mt-5'>" + noresultText + "</p>");
                                 $(".no-result").css("display", "block");
                                 $(".result-table .table-body").html("");
                             }
                             else {
                                 $(".no-result").css("display", "none");
                                 $(".showMoreFlights").show();
                                 var content = "";
                                 var progressItemContent = "";
                                 var progressItemContentMobile = "";

                                 for (var i = 0; i < data.result.data.flights.length; i++) {
                                     var item = data.result.data.flights[i];
                                     var scheduledDatetime = new Date(item.scheduledDatetime);
                                     var estimatedDatetime = new Date(item.estimatedDatetime);
                                     var gateMapLink = "/en/airport/maps/airport-map/" + "?placeId=fd536e57-abe1-46de-a4f0-bc309cfa327c&endStoreId=";
                                     var carouselMapLink = "/en/airport/maps/airport-map/" + "?placeId=fd536e57-abe1-46de-a4f0-bc309cfa327c&endStoreId=CAROUSEL-";

                                     var statusClass = "";
                                     //var remarkCode=item.airsideRemarkCode;
                                     var remarkCode = item.remarkCode;
                                     switch (remarkCode) {
                                         case "LAN":
                                         case "GTO":
                                         case "SCT":
                                             statusClass = "yesil";
                                             break;
                                         case "DEL":
                                         case "BEN":
                                         case "OFB":
                                         case "BST":
                                             statusClass = "turuncu";
                                             break;
                                         case "DX":
                                         case "FCL":
                                             statusClass = "kirmizi";
                                             break;
                                         default:
                                             statusClass = "other";
                                             break;
                                     }
                                     progressItemContentMobile += `<div class="item ${statusClass}">
                                                                                              <div class="head">
                                                                                                      <div class="time">
                                                                                                          <p class="delay">${scheduledDatetime.toLocaleTimeString("tr-TR", { hour: '2-digit', minute: '2-digit' })}</p>
                                                                                                          <p class="new">${estimatedDatetime.toLocaleTimeString("tr-TR", { hour: '2-digit', minute: '2-digit' })}</p>
                                                                                                      </div>
                                                                                                       <div class="middle">
                                                                                                                  <p class="loc">${flightType == 0 ? item.fromCityName : item.toCityName}</p>
                                                                                                                  <ul>
                                                                                                                          <li>${item.flightNumber}</li>`;
                                     if (item.codeshare != null) {
                                         for (var j = 0; j < item.codeshare.length; j++) {
                                             progressItemContentMobile += `<li> ${item.codeshare[j]} </li>`;
                                         }
                                     }

                                     progressItemContentMobile += `</ul>
                                                                                                      </div>
                                                                                                                      <div class="gate-status ${statusClass}">
                                                                                                                          <p> ${item.remark != null ? item.remark : '-'}</p>
                                                                                                          <span class="icon-chevron-down"></span>
                                                                                                      </div>
                                                                                                  </div>
                                                                                                  <div class="content">
                                                                                                      <div class="top">
                                                                                                          <div class="date">
                                                                                                               <p>${ddMMM(scheduledDatetime)}</p>
                                                                                                          </div>
                                                                                                          <div class="logo-slick">
                                                                                                              <div class="slick-item">
                                                                                                                    <img src="https://api.igairport.aero/iga/v2/Files/Images/airlines/${item.airlineCode}.png" alt="${item.airlineName}" class="w-100">
                                                                                                              </div>

                                                                                                          </div>
                                                                                                      </div>
                                                                                                           <div class="bottom">`;

                                     if (flightType == 0) {
                                         progressItemContentMobile += `<div class="carousel">
                                                                                                                                     <p>Carousel</p>
                                                                                                                                         <p>${item.carousel != null ? item.carousel : '-'}</p> </div>
                                                                                                                               <div class="gate">
                                                                                                                                     <p>Gate</p>
                                                                                                                       <p>${item.gate != null ? item.gate : '-'}</p>

                                                                                                                   </div>`;
                                     }
                                     else {
                                         progressItemContentMobile += `  <div class="counter">
                                                                                                                  <p>Check-in Counter</p>
                                                                                                          <p>${item.counter != null ? item.counter : '-'}</p>
                                                                                                   </div>
                                                                                              <div class="gate">
                                                                                                             <p>Gate</p>
                                                                                                               <p>${item.gate != null ? item.gate : '-'}</p>
                                                                                                   </div>`;
                                     }
                                     progressItemContentMobile += `</div>
                                                                                                  </div>
                                                                                              </div>`;


                                     progressItemContent +=
                                         `<div class="table-row table-item">
                                                                              <div class="table-cell date">
                                                                                      <div class="mobile-title">
                                                                                          <span>Date</span>
                                                                                  </div>
                                                                                  <div class="date">
                                                                                  <span>${ddMMM(scheduledDatetime)}</span>
                                                                                      </div>
                                                                              </div>
                                                                              <div class="table-cell time">
                                                                                  <div class="mobile-title">
                                                                                      <span>Planned  /  Estimated</span>
                                                                                  </div>
                                                                                  <div class="time-list">
                                                                                      <div class="time">
                                                                                          <span>${scheduledDatetime.toLocaleTimeString("tr-TR", { hour: '2-digit', minute: '2-digit' })}</span>
                                                                                      </div>
                                                                                      <div class="time">
                                                                                          <span>${estimatedDatetime.toLocaleTimeString("tr-TR", { hour: '2-digit', minute: '2-digit' })}</span>
                                                                                      </div>
                                                                                  </div>
                                                                              </div>
                                                                              <div class="table-cell airline" >
                                                                                  <div class="mobile-title">
                                                                                      <span>Airline</span>
                                                                                  </div>
                                                                              <div class="airline-img airline" style="width:100px">
                                                                                          <div class="airline-code-slick">
                                                                                              <img src="https://api.igairport.aero/iga/v2/Files/Images/airlines/${item.airlineCode}.png"
                                                                                                                                                                                                                                                                      alt="${item.airlineName}"  width="150px"/>`;
                                     if (item.airlineCodeList != null) {
                                         for (var j = 1; j < item.airlineCodeList.length; j++) {
                                             progressItemContent += ` <img src="https://api.igairport.aero/iga/v2/Files/Images/airlines/${item.airlineCodeList[j]}.png" alt="${item.airlineCodeList[j]}}" width="150px"  />`;

                                         }
                                     }

                                     progressItemContent += `</div>
                                                                                                              </div>
                                                                                                      </div>
                                                                                                      <div class="table-cell flight-number">
                                                                                                          <div class="mobile-title">
                                                                                                              <span >Flight Number</span>
                                                                                                          </div>
                                                                                                          <div class="flight-number" style="width:100px">
                                                                                                          <div class="flight-code-slick">
                                                                                                              <span class="codeshare flightNumberItem-0">${item.flightNumber}</span>`;
                                     if (item.codeshare != null) {
                                         for (var j = 0; j < item.codeshare.length; j++) {
                                             progressItemContent += `<span class="codeshare flightNumberItem-${j}">${item.codeshare[j]}</span>`;
                                         }
                                     }


                                     progressItemContent += `  </div></div>
                                                                                          </div>

                                                                                          <div class="table-cell arrival">
                                                                                              <div class="mobile-title">
                                                                                                  <span>
                                                                                                  ${flightType == 0? 'Departure' : 'Arrival'}

                                                                                                  </span>
                                                                                              </div>
                                                                                              <div class="arrival">

                                                                                                  <span>${flightType == 0? item.fromCityName : item.toCityName}</span>
                                                                                              </div>
                                                                                          </div>
                                                                                          <div class="table-cell  counter">
                                                                                              <div class="mobile-title">
                                                                                                  <span>Check-in Counter</span>
                                                                                              </div>
                                                                                              <div class="counter">
                                                                                                  <span>${item.counter != null ? item.counter : '-'}</span>
                                                                                              </div>
                                                                                          </div>
                                                                                          <div class="table-cell gate">
                                                                                              <div class="mobile-title">
                                                                                                  <span>Gate</span>
                                                                                              </div>
                                                                                              <div class="gate">`;
                                     if (flightType == 0) {
                                         progressItemContent += `<span >${item.gate != null ? item.gate : '-'}</span>`;
                                     }
                                     else {
                                         progressItemContent += `<a href="${gateMapLink}${item.gate}" target="_blank">
                                                                                                                                                                                                                                                       <span class="text">${item.gate != null ? item.gate : '-'}</span></a>`;
                                     }
                                     progressItemContent += `</div>
                                                                          </div>
                                                                          <div class="table-cell carousel">
                                                                              <div class="mobile-title">
                                                                                  <span>Carousel</span>
                                                                              </div>
                                                                              <div class="carousel">
                                                                                  <a href="${carouselMapLink}${item.carousel}"  target="_blank" class="hyperlink">
                                                                                  <span class="text">${item.carousel != null ? item.carousel : '-'}</span></a>
                                                                              </div>
                                                                          </div>
                                                                          <div class="table-cell status ${statusClass}">
                                                                          <div class="mobile-title status">
                                                                                  <span>Current Status</span>
                                                                              </div>
                                                                              <div class="status ${statusClass}">
                                                                                  <span>
                                                                                  ${item.remark != null ? item.remark : '-'}
                                                                                  </span>
                                                                              </div>
                                                                          </div>

                                                              </div>`;
                                 }
                                 if (issearch == 1 && prevFlightPage == 0) {
                                     $(".result-table .table-body").html("");
                                 }

                                 if (prevFlightPage > 0 && clickedButton == "prevFlight") {
                                     prevStartDate = data.result.NewStartDate;

                                     var listHtml = $(".result-table .table-body").html();
                                     $(".result-table .table-body").html("");
                                     $(".result-table .table-body").append(progressItemContent + listHtml).hide().fadeIn(1000);

                                     var listHtmlMobile = $(".mobile-result-table").html();
                                     $(".mobile-result-table").html("");
                                     $(".mobile-result-table").append(progressItemContentMobile + listHtmlMobile).hide().fadeIn(1000);
                                 }
                                 else {
                                     $(".result-table .table-body").append(progressItemContent).hide().fadeIn(1000);
                                     $(".mobile-result-table").append(progressItemContentMobile).hide().fadeIn(1000);
                                 }

                                 $('.flight-code-slick').slick({ dots: false, infinite: false, arrows: false, autoplay: true, autoplaySpeed: 2500 });
                                 $('.airline-code-slick').slick({ dots: false, infinite: false, arrows: false, autoplay: true, autoplaySpeed: 2500 });

                                 if (flightType == 0) {
                                     $(".counter").attr("style", "display:none");
                                     $(".carousel").removeAttr("style");
                                 }
                                 else {
                                     $(".counter").removeAttr("style");
                                     $(".carousel").attr("style", "display:none");
                                 }


                             }
                         }
                         else {

                         }

                         clickedButton = "";
                         $("#loading").hide();
                     }
                 });
             }

             function RefreshBoard() {
                 pageNumber = 1;
                 clickedButton = "";
                 prevFlightPage = 0;
                 $(".result-table .table-body").html("");
                 $(".mobile-result-table").html("");

             }
