<script type="text/javascript">
             $(document).ready(function () {
                 $('.checkpoint-section').hide();
                 GetQmsData();
             });

             var minuteText = "Minute";
             var pluralMinuteText = "Minutes";
             function GetQmsData() {
                 $.ajax({
                     type: "GET",
                     contentType: "application/json",
                     dataType: 'json',
                     url: "/umbraco/api/Checkpoint/GetWaitingTimes?culture=en-US",
                     success: function (data) {

                         if (data.status) {

                             var gateClassName = data.result.gateWaitTimeClassName.split("-")[1];

                             var passClassName = data.result.passQueueWaitTimeClassName.split("-")[1];

                             var domClassName = data.result.domQueueWaitTimeClassName.split("-")[1];


                             $('.checkpoint-section').show();
                             //$('#gate-time').text(data.result.gateWaitTime);
                             $('#gate-time-name').html("Busiest entrance" + " ( " + data.result.gateWaitTimeName + " )");
                             $('#gate-time-class').addClass(data.result.gateWaitTimeClassName);
                             $('#gate-time-class').html('<span id="gate-time">' + data.result.gateWaitTime + '</span>' + (data.result.gateWaitTime > 1 ? pluralMinuteText : minuteText));
                             $('#gate-status').addClass(gateClassName);
                             //$('#passQueue-time').text(data.result.passQueueWaitTime);
                             $('#passQueue-time-name').html("Busiest entrance" + " ( " + data.result.passQueueWaitTimeName + " )");
                             $('#passQueue-time-class').addClass(data.result.passQueueWaitTimeClassName);
                             $('#passQueue-time-class').html('<span id="passQueue-time">' + data.result.passQueueWaitTime + '</span>' + (data.result.passQueueWaitTime > 1 ? pluralMinuteText : minuteText));
                             $('#pass-status').addClass(passClassName);
                             //$('#domQueue-time').text(data.result.domQueueWaitTime);
                             $('#domQueue-time-name').html("Busiest entrance" + " ( " + data.result.domQueueWaitTimeName + " )");
                             $('#domQueue-time-class').addClass(data.result.domQueueWaitTimeClassName);
                             $('#domQueue-time-class').html('<span id="domQueue-time">' + data.result.domQueueWaitTime + '</span>' + (data.result.domQueueWaitTime > 1 ? pluralMinuteText : minuteText));
                             $('#dom-status').addClass(domClassName);
                         }
                         else {
                             $('.checkpoint-section').hide();
                         }
                     },
                     failure: function (data) {
                         $('.checkpoint-section').hide();
                     },
                     error: function (data) {
                         $('.checkpoint-section').hide();
                     }
                 });
             };
    </script>
<script>

                 $(function() {
                     $("#txtSearch").on('paste', function(e) {
                         var _this = $(this);
                         // we can access input value on paste after some time
                         setTimeout(function() {
                             // access element value
                             var txtLength = _this.val().length;
                             if (txtLength >= 3) {
                                 if ($('.search-card-slick .slick-list').length > 0) {
                                     $('.search-card-slick').slick('destroy');
                                 }

                                 GetPopulerSearches();
                                 GetHeaderSearchResult();
                             }
                             else {
                                 $("#populer-searches-template").empty();
                                 $("#related-categories-template").empty();
                                 $("#card-result-template").empty();
                                 $(".see-all-populer").empty();
                                 $(".see-all-related").empty();
                                 $(".category-title-result span").empty();
                                 $(".populer-searches-title span").empty();
                             }
                         }, 1);


                     })



                     $("#txtSearch").on('keydown', function(e) {

                         var txtLength = $(this).val().length;
                         var code = e.keyCode ? e.keyCode : e.which;

                         if (code == 13) {
                             RedirectToSearchPage();


                         }
                         else if (txtLength >= 3) {
                             if ($('.search-card-slick .slick-list').length > 0) {
                                 $('.search-card-slick').slick('destroy');
                             }

                             GetPopulerSearches();
                             GetHeaderSearchResult();
                         }
                         else if (txtLength < 3) {
                             $("#populer-searches-template").empty();
                             $("#related-categories-template").empty();
                             $("#card-result-template").empty();
                             $(".see-all-populer").empty();
                             $(".see-all-related").empty();
                             $(".category-title-result span").empty();
                             $(".populer-searches-title span").empty();
                         }
                     })
                 });


                 function GetPopulerSearches() {

                     $.ajax({
                         type: "GET",
                         url: "/umbraco/Surface/Search/GetPopulerSearches",
                         contentType: "application/json; charset=utf-8",
                         data: { culture: "en-US" },
                         success: function(result) {
                             if (result != null) {

                                 var seeAllText = 'See All';

                                 $("#populer-searches-template").html("");
                                 //$('.populer-searches-title').html("<span>Popüler Arama</span>");
                                 $('.populer-searches-title').html("Populer Searches");
                                 if (result.categories.length > 0) {

                                     let relCatTemplate = "";
                                     $('.see-all-populer').html('<a href="javascript:;" onclick="RedirectToPopulerSearchPage()"><span class="text">' + seeAllText + '</span><span class="icon icon-chevron-right"></span></a>');
                                     $.each(result.categories, function(index, value) {

                                         if (value.category != '') {
                                             relCatTemplate += `<li><a href="#">${value.category} (${value.resultList.length})</a></li>`
                                         }
                                     });

                                     $("#populer-searches-template").append(relCatTemplate);
                                 }
                                 else {

                                     $('#populer-searches-template').empty();

                                     $('.populer-searches-title span').empty();

                                     $('.see-all-populer').empty();
                                 }
                             }
                         }
                     });
                 }

                 function GetHeaderSearchResult() {

                     var searchKey = $.trim($("#txtSearch").val());

                     var culture = 'en-US';

                     var searchPage = '/en/search/';

                     var seeAllText = 'See All';
                     var readDetails = 'Details';


                     $.ajax({
                         type: "GET",
                         url: "/umbraco/Surface/Search/HeaderSearch",
                         contentType: "application/json; charset=utf-8",
                         data: { searchKey: searchKey, culture: culture },
                         success: function(result) {

                             $('.category-title-result span').empty();

                             $('.see-all-related').empty();

                             if (result != null) {

                                 $("#related-categories-template").html("");
                                 if (result.categories.length > 0) {
                                     $(".search-result").find(".result-item").show();
                                     $(".search-result").find(".no-result").hide();

                                     let relCatTemplate = "";

                                     $('.category-title-result span').text("Related Categories");

                                     $('.see-all-related').append(`<a href="javascript:;" onclick="RedirectToSearchPage()"><span class="text">${seeAllText}</span><span class="icon icon-chevron-right"></span></a>`);


                                     $.each(result.categories, function(index, value) {

                                         if (value.category != '') {
                                             relCatTemplate += `<li><a href="#">${value.category} (${value.resultList.length})</a></li>`
                                         }
                                     });

                                     $("#related-categories-template").append(relCatTemplate);

                                 }
                                 else {

                                     $(".search-result").find(".result-item").hide();
                                     $(".search-result").find(".no-result").show();

                                     $('#related-categories-template').empty();

                                     $('.category-title-result span').empty();

                                     $('.see-all-related').empty();
                                 }

                                 if (result.recommended.length > 0) {

                                     let recTemplate = "";

                                     $('#card-result-template').empty();

                                     $.each(result.recommended, function(index, value) {

                                         var tag = value.tag;
                                         var title = value.title;
                                         var contentType = value.contentType;

                                         if (title == null || title == "") {
                                             title = value.name;
                                         }

                                         recTemplate += `<div class="result-item">
                                                                 <div class="default-card">
                                                                     <div class="card-content">
                                                                         <div class="content-body">
                                                                             <div class="category-title">
                                                                             <span>${tag}</span>
                                                                             </div>
                                                                             <div class="main-title">
                                                                                 <span>${title}</span>
                                                                             </div>`;
                                         if (contentType == "blogPage") {
                                             recTemplate += `<div class="icons d-flex mt-3">
                                                                                                         <div class="view mr-3">
                                                                                                         <span class="icon-eye"></span>
                                                                                                         <span class="text">${value.viewCount}</span>
                                                                                                         </div>
                                                                                                     </div>`;
                                         }
                                         recTemplate += ` </div>
                                                                         <div class="content-footer">
                                                                         <div class="read-details black">
                                                                                 <a href="${value.url}"><span class="text">${readDetails}</span><span class="icon icon-chevron-right"></span></a>
                                                                         </div>
                                                                         </div>
                                                                     </div>
                                                                     <div class="card-img"><img src="${value.imageUrl}?format=webp" alt="" />
                                                                     </div>
                                                                 </div>
                                                                 </div>`;

                                     });

                                     $('#card-result-template').append(recTemplate);

                                     $('.search-card-slick').not('.slick-initialized').slick({
                                         dots: false,
                                         infinite: false,
                                         variableWidth: true,
                                         arrows: false,
                                         swipe: true,
                                         responsive: [{
                                             breakpoint: 980,
                                             settings: {
                                                 swipe: true
                                             }
                                         }]
                                     });
                                     $('.search-card-slick').slick('refresh');
                                     //if ($(window).width() < 767) {
                                     //    $('.search-carousel').slick({
                                     //        dots: false,
                                     //        infinite: false,
                                     //        variableWidth: true,
                                     //        slidesToShow: 2,
                                     //        slidesToScroll: 2,
                                     //        arrows: false,
                                     //        swipe: false

                                     //    });
                                     //}
                                 }
                                 else {
                                     $('#card-result-template').empty();
                                 }
                             }
                             else {

                                 $(".search-result").find(".result-item").hide();
                                 $(".search-result").find(".no-result").show();
                                 $('#related-categories-template').empty();

                                 $('#card-result-template').empty();
                             }
                             setTimeout(function() {

                             }, 1000)

                         }
                     });



                 }

                 function RedirectToSearchPage() {

                     var searchKey = $.trim($("#txtSearch").val());

                     var searchPage = '/en/search/';

                     window.location.href = searchPage + "?searchKey=" + searchKey;

                 }

                 function RedirectToPopulerSearchPage() {

                     var searchPage = '/en/search/';
                     window.location.href = searchPage;

                 }

