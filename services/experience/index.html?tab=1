        $(document).ready(function() {

          
                    $.ajax({
                        url: "/umbraco/api/General/SetPageViewCounter",
                        dataType: "JSON",
                        type: "POST",
                        data: {
                        contentId: 2552,
                         culture:"en-US"
                        },
                        success: function(data) {
                        }
                    });
            GetWeatherData();
         
